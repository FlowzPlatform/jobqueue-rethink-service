{
  "plugins" : {
    "createPattern": "role:job,cmd:create",
    "findPattern": "role:job,cmd:findjob",
    "queuePattern": "role:job,cmd:queue",
    "updatePattern": "role:job,cmd:update",
    "subscriptionCreatePattern": "role:subscription,cmd:created"
  },
  "defaultConnection" : {
    "host": "localhost",
    "port": 28015,
    "db": "jobQueue"
  },
  "defaultQueue" : {
    "name": "RegistrationEmail",
    "concurrency": 5
  },
  "defaultCreateJob" : {
    "priority": "normal",
    "timeout": 300000,
    "retrymax": 3,
    "retrydelay": 600000
  },
  "web-option": {
    "port": 5000,
    "urlPrefix": "/job"
  },
  "defaultSubscription": {
    "enable": true
  },
  "registerWorker": {
    "table": "registerWorkers",
    "port": 3000,
    "getJobModuleApiURL": "http://localhost:3000/job-module/"
  },
  "symmetricWorker": {
    "port" : 9001,
    "table" : "symmetricWorkers",
    "executeWorkerURL": "http://localhost:9001/execute-worker",
    "waitingThreshold": 0.40,
    "increaseWorker": 10,
    "maxWorker": 10,
    "childProcessFile": "start-child-worker.js"
  },
  "pino": {
    "timestamp": false,
    "prettyPrint": {
      "forceColor": true
    }
  }
}
